{
  "name": "mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "eslint src/ && node node_modules/react-native/local-cli/cli.js start",
    "ios": "./node_modules/.bin/eslint src/** --fix && react-native run-ios && ./node_modules/.bin/nodemon --exec 'npm run lint'",
    "android": "./node_modules/.bin/eslint src/** --fix && react-native run-android && ./node_modules/.bin/nodemon --exec 'npm run lint'",
    "test": "jest",
    "lint": "./node_modules/.bin/eslint src/** --fix"
  },
  "dependencies": {
    "react": "15.4.2",
    "react-native": "0.42.3",
    "react-native-fbsdk": "^0.5.0",
    "react-navigation": "^1.0.0-beta.7",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.0",
    "redux-promise-middleware": "^4.2.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-jest": "19.0.0",
    "babel-preset-react-native": "1.9.1",
    "eslint": "^3.19.0",
    "eslint-plugin-class-property": "^1.0.3",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-react-native": "^2.3.1",
    "jest": "19.0.2",
    "nodemon": "^1.11.0",
    "react-test-renderer": "15.4.2"
  },
  "jest": {
    "preset": "react-native"
  }
